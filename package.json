{
  "name": "gc-backup-app",
  "productName": "gc-backup-app",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "SET NODE_ENV=development && electron-forge start",
    "start:gulp": "yarn run gulp",
    "start:db": "SET NODE_ENV=development && electron-forge start --inspect-electron",
    "package": "SET NODE_ENV=production && electron-forge package",
    "make": "SET NODE_ENV=production && electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "kervy",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "asar": {
          "unpack": "**/config/database.sqlite"
        },
        "prune": true
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "gc_backup_app"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "electronRebuildConfig": {},
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "devContentSecurityPolicy": "connect-src 'self' http://localhost:8083 'unsafe-eval'",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/public/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/server/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-react": "^7.16.7",
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@electron-forge/plugin-auto-unpack-natives": "^6.0.0-beta.63",
    "@electron-forge/plugin-webpack": "6.0.0-beta.63",
    "@vercel/webpack-asset-relocator-loader": "1.7.0",
    "babel-loader": "^8.2.3",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.0.0",
    "dotenv": "^16.0.0",
    "electron": "17.1.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.7",
    "gulp": "^4.0.2",
    "gulp-sass": "^5.1.0",
    "node-loader": "^2.0.0",
    "sass": "^1.49.9",
    "style-loader": "^3.0.0",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.5.0",
    "@mui/material": "^5.4.3",
    "@reduxjs/toolkit": "^1.7.2",
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "electron-json-settings-store": "^1.0.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^8.0.1",
    "express": "^4.17.3",
    "framer-motion": "^6.2.8",
    "helmet": "^5.0.2",
    "http-errors": "^2.0.0",
    "mongodb": "^4.4.0",
    "node-cron": "^3.0.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "sequelize": "^6.17.0",
    "sqlite3": "^5.0.2"
  }
}
